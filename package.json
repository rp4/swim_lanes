{
  "name": "swimlanes",
  "version": "1.0.0",
  "description": "Interactive swim lane diagram visualizer for business process modeling",
  "keywords": [
    "swimlane",
    "diagram",
    "visualization",
    "business-process",
    "bpm",
    "workflow",
    "flowchart",
    "process-mapping",
    "svg",
    "interactive"
  ],
  "homepage": "https://github.com/rp4/swimlanes#readme",
  "bugs": {
    "url": "https://github.com/rp4/swimlanes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rp4/swimlanes.git"
  },
  "license": "MIT",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "type": "module",
  "private": true,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "semantic-release",
    "docs": "typedoc --out docs/api src",
    "clean": "rm -rf dist coverage docs/api"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@playwright/test": "^1.40.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-legacy": "^7.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "semantic-release": "^22.0.12",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0"
  }
}
